            }
            else {
                $("#KValueAfter").html("<b>K value was not found</b>");
            }
        }
    }
    //output values to display

   
    valMessage = userKvalue + " was found by adding " + firstNumber + " and " + secondNumber + "."

    $("#soafAns").text(valMessage);
												
