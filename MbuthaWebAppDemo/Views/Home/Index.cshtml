    var secondNumber;

    var user = [
        +$("#soafNum1").val(),
        +$("#soafNum2").val(),
        +$("#soafNum3").val(),
        +$("#soafNum4").val(),
        +$("#soafNum5").val(),
        +$("#soafNum6").val()
